declare
SemanticStack = {NewCell nil}
Environment = {Dictionary.new}
ProgramStatements = [[nop] [nop] [nop]]
SemanticStack := {Append [semanticStatement(ProgramStatements Environment)] @SemanticStack}

declare 
fun {Interpretor}
   local Stmt Env in
      case @SemanticStack of nil then
	 Stmt = nil
	 Env = nil
      else
	 Stmt = @SemanticStack.1.1
	 Environment = @SemanticStack.1.2
	 SemanticStack := @SemanticStack.2
      end
      case Stmt of nil then nil
      [] nop|nil then
	 {Browse nop}
	 {Interpretor}
      end
   end
end
%{Browse {Interpretor}}
{Browse @SemanticStack}